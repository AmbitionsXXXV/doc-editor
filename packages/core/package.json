{
	"name": "@doc-editor/core",
	"private": false,
	"sideEffects": ["*.css"],
	"license": "MIT",
	"version": "1.19.0",
	"keywords": [
		"doc-editor",
		"editor",
		"typescript",
		"prosemirror",
		"rich-text-editor",
		"notion",
		"yjs",
		"block-based",
		"tiptap"
	],
	"type": "module",
	"source": "src/index.ts",
	"main": "build/cjs/index.cjs",
	"module": "build/esm/index.js",
	"exports": {
		".": {
			"import": "./build/esm/index.js"
		},
		"./style.css": {
			"import": "./build/esm/index.css"
		}
	},
	"scripts": {
		"dev": "rslib build --watch",
		"build": "rslib build",
		"lint": "biome check --write ./src",
		"format": "biome format --write ./src",
		"clean": "node ../../tools/clean.js dist node_modules .turbo",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@emoji-mart/data": "^1.2.1",
