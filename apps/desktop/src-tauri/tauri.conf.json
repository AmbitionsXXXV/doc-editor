{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "etc-editor",
	"version": "0.1.0",
	"identifier": "com.etc-editor.docs",
	"build": {
		"beforeDevCommand": "echo 1",
		"devUrl": "http://localhost:5173",
		"beforeBuildCommand": "echo 2",
		"frontendDist": "../../web/build/client/"
	},
	"app": {
		"windows": [
			{
				"title": "etc-editor",
				"width": 1500,
				"height": 900,
				"theme": "Light"
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
